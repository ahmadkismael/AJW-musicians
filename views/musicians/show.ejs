<%- include('../partials/headAndNav') %>


<div class="container">
  <div class="card align-items-center text-center my-2 py-2">
      <img src= <%= musician.img %> class="card-img-top" alt="...">
      <div class="card-body">
        <h5 class="card-title"><%= musician.name %></h5>
        <p class="card-text">Instrument: <%= musician.instrument %></p>
        <a href="/musicians/<%= musician._id%>/edit" class="btn btn-primary">Edit</a>
        <form class="my-2 py-2" method="POST" action="/musicians/<%= musician._id %>?_method=DELETE">
          <input class="btn btn-primary" type="submit" value="Delete Me">
        </form>
        <!-- <a href="/musicians/<%= musician._id %>" class="btn btn-primary">Delete</a> -->
      </div> 
    </div>

</div>


<%- include('../partials/footer') %>